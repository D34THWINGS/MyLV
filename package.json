  "scripts": {
    "build": "webpack",
    "changelog": "conventional-changelog -c .conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dist": "webpack --env.production",
    "start": "webpack-dev-server --hot",
    "start:server": "nodemon ./src/server -e js,pug,css",
    "start:emails": "browser-sync start --proxy localhost:8001 --startPath /api/emails/render?template=holidaysRequest --files src/server/plugins/mail/**/*.{css,pug}",
    "test": "npm run lint && npm run test:unit",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "test:unit": "BABEL_ENV=test jest",
    "test:unit:watch": "jest --watch",
    "version": "npm run changelog && git add CHANGELOG.md",
    "lint:fix": "eslint --fix \"**/*.{js,jsx}\""
  },
